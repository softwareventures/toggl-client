{
  "private": true,
  "name": "@softwareventures/toggl-client",
  "version": "0.0.0-development",
  "description": "Client for Toggl API",
  "keywords": [
    "toggl",
    "api",
    "client",
    "TypeScript"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/toggl-client",
  "bugs": "https://github.com/softwareventures/toggl-client",
  "repository": "github:softwareventures/toggl-client",
  "license": "ISC",
  "scripts": {
    "fix": "eslint . --fix && prettier --write .",
    "lint": "eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "dependencies": {
    "@types/node": "^13.7.7",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "3.4.2",
    "@softwareventures/prettier-config": "1.0.2",
    "@softwareventures/semantic-release-config": "1.1.1",
    "@softwareventures/tsconfig": "5.0.1",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.20.0",
    "prettier": "2.2.1",
    "semantic-release": "17.3.9",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
