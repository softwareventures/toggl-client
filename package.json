{
  "private": true,
  "name": "@softwareventures/toggl-client",
  "version": "0.0.0-development",
  "description": "Client for Toggl API",
  "keywords": [
    "toggl",
    "api",
    "client",
    "TypeScript"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/toggl",
  "bugs": "https://github.com/softwareventures/toggl",
  "repository": "github:softwareventures/toggl",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "dependencies": {
    "@types/node": "^13.7.7",
    "tslib": "1.11.1"
  },
  "devDependencies": {
    "@softwareventures/tsconfig": "2.2.0",
    "@softwareventures/tslint-rules": "0.8.1",
    "ava": "3.8.1",
    "cz-conventional-changelog": "3.1.1",
    "semantic-release": "17.0.7",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
