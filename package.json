{
  "private": true,
  "name": "@softwareventures/toggl-client",
  "version": "0.0.0-development",
  "description": "Client for Toggl API",
  "keywords": [
    "TypeScript",
    "api",
    "client",
    "toggl"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/toggl-client",
  "bugs": "https://github.com/softwareventures/toggl-client",
  "repository": "github:softwareventures/toggl-client",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "dependencies": {
    "@types/node": "14.18.34",
    "tslib": "2.4.1",
    "unknown": "^0.2.5"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "7.1.0",
    "@softwareventures/prettier-config": "4.0.0",
    "@softwareventures/semantic-release-config": "3.1.0",
    "@softwareventures/tsconfig": "7.0.0",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "ava": "5.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-sonarjs": "0.17.0",
    "prettier": "2.8.1",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
